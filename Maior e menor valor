#include <stdio.h>
#include <stdlib.h>


int main()
{
    int valor1, valor2;
    printf("Digite um valor: ");
    scanf("%i", &valor1);
    printf("\nDigite outro valor: ");
     scanf("%i", &valor2);
     
    if(valor1 > valor2){
        printf("O valor %i e maior", valor1);
    }else{
       printf("O valor %i e maior", valor2);
    }
    return 0;
}
