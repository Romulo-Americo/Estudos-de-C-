#include <stdio.h>
#include <stdlib.h>

int main()
{
  printf("Tabuada");
  
  int i = 1;
  int numero, resultado;
  printf("Digite um numero para ver a sua tabuada: ");
  scanf("%i", &numero);
  while (i<=11){
    printf("%i\n", resultado);
    resultado = i * numero;
    i += 1 ;
  }
    
  return 0; 
}
